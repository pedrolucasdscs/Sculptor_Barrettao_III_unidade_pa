<div class="line"><span class="lineno">   18</span>  </div>
<div class="line"><span class="lineno">   19</span>  <span class="keywordflow">for</span>(i = (xcenter - rx);i &lt; (xcenter + rx);i++){</div>
<div class="line"><span class="lineno">   20</span>    <span class="keywordflow">for</span>(j = (ycenter - ry);j &lt; (ycenter + ry);j++){</div>
<div class="line"><span class="lineno">   21</span>      <span class="keywordflow">for</span>(k = (zcenter - rz);k &lt; (zcenter + rz);k++){</div>
<div class="line"><span class="lineno">   22</span> </div>
<div class="line"><span class="lineno">   23</span>        SegX= pow(i-xcenter,2) / pow (rx,2); </div>
<div class="line"><span class="lineno">   24</span>        SegY= pow(j-ycenter,2) / pow (ry,2);</div>
<div class="line"><span class="lineno">   25</span>        SegZ= pow(k-zcenter,2) / pow (rz,2);</div>
<div class="line"><span class="lineno">   26</span> </div>
<div class="line"><span class="lineno">   27</span>        <span class="keywordflow">if</span>((SegX + SegY + SegZ)&lt;=1){</div>
<div class="line"><span class="lineno">   28</span>          s.<a class="code hl_function" href="class_sculptor.html#ad9d714a35fc8ae16d06eb5df37c3493c">cutVoxel</a>(i,j,k);</div>
<div class="line"><span class="lineno">   29</span>        }</div>
<div class="line"><span class="lineno">   30</span>      }</div>
<div class="line"><span class="lineno">   31</span>    }</div>
<div class="line"><span class="lineno">   32</span>  }</div>
<div class="line"><span class="lineno">   33</span>}</div>
